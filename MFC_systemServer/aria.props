    <IncludePath>C:\Program Files\MobileRobots\Aria\include;$(IncludePath)</IncludePath>
    <LibraryPath Condition="'$(Platform)'=='Win32'">C:\Program Files\MobileRobots\Aria\lib;$(LibraryPath)</LibraryPath>
    <ExecutablePath>C:\Program Files\MobileRobots\Aria\bin;$(ExecutablePath)</ExecutablePath>
    <SourcePath>C:\Program Files\MobileRobots\Aria\src;$(SourcePath)</SourcePath>
  </PropertyGroup>
  <ItemDefinitionGroup>
