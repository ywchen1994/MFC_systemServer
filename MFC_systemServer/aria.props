      <AdditionalDependencies>AriaDebugVC12.lib;ArNetworkingDebugVC12.lib;%(AdditionalDependencies)</AdditionalDependencies>
    </Link>
    <Link Condition="'$(Configuration)'=='Release'">
      <AdditionalDependencies>AriaVC12.lib;ArNetworkingVC12.lib;%(AdditionalDependencies)</AdditionalDependencies>
    </Link>
  </ItemDefinitionGroup>
  <ItemGroup />
</Project>